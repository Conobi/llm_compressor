{
  "name": "notebox-compressor",
  "version": "0.1.0",
  "description": "LLM-based lossless text compression using RWKV and arithmetic coding",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist",
    "assets"
  ],
  "scripts": {
    "build": "tsup",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint src --ext .ts",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "compression",
    "llm",
    "rwkv",
    "arithmetic-coding",
    "text-compression",
    "lossless"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "onnxruntime-web": "^1.20.1"
  },
  "devDependencies": {
    "@types/node": "^22.10.7",
    "tsup": "^8.3.5",
    "tsx": "^4.7.0",
    "typescript": "^5.7.3",
    "vitest": "^2.1.8"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
